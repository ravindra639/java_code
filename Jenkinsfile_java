pipeline {
	parameters {
			text(name: 'REPO', defaultValue: 'welcome', description: '')
		}
    agent any 
    stages {
        stage('Build') { 
            steps {
                bat '''
				echo Build Stage
				mvn clean compile
				'''
            }
        }
		stage('Deploy') { 
            steps {
                bat '''
				echo Deploy Stage
				mvn clean install
				'''
            }
        }
    }
}
